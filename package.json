    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "react-hot-loader": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.20.3",
